# Obter a lista de animais - teste de GET
GET http://localhost:3000/animais
Content-Type: application/json
###

# Inserir novo animal
POST http://localhost:3000/animais
Content-Type: application/json

{
 "tipo": "cachorro",
 "nome": "Bela",
 "idade": 2
}
###

# Alterar um animal
PUT http://localhost:3000/animais
Content-Type: application/json

{
 "_id": "6227e7c50ba122eaa6f3830a",
 "tipo": "cachorro",
 "nome": "Bela",
 "idade": 3
}
###

# Excluir um animal pelo id
DELETE http://localhost:3000/animais/6227e7c50ba122eaa6f3830a
Content-Type: application/json
###

# Obter animal pelo id
GET http://localhost:3000/animais/6227e7c50ba122eaa6f3830a
Content-Type: application/json
###

# Obter animais filtrando
GET http://localhost:3000/animais/filtro/cachorro
Content-Type: application/json
###